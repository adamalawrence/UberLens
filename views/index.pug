extends layout

block content

  

  
    input#autocomplete(placeholder='Enter your address', onfocus='geolocate()', type='text')




        

    .container
      img(src='/images/UberLens-logo-inline-cropped.png', margin-bottom="100px")
      br
      .header.clearfix
        nav
          ul.nav.nav-pills.pull-right
            li(role='presentation')
              a(href='#') About
        //- a.navbar-left(href='#')
        //-   img(src='/Users/adamlawrence/Developer/UberLens/myapp/views/UberLens-logo-inline.png')

      //-   h3.text-muted UberLens.io 
      .jumbotron
        //- h2 Jumbotron heading
        form
          .form-group
            label(for='exampleInputEmail1') Where you at
            input#exampleInputEmail1.form-control(type='email', placeholder='e.g. 1999 Burdett Ave, Troy NY, 12180')
          .form-group
            label(for='exampleInputPassword1') Where you're goin
            input#exampleInputPassword1.form-control(type='password', placeholder='e.g. Molly Moon\'s ice cream')
          .form-group
            label(for='exampleInputPassword1') How far are you willing to walk to your destination from your Uber drop off?
            br
            label.radio-inline
              input#inlineRadio1(type='radio', name='inlineRadioOptions', value='option1')
              |  5 minutes
            label.radio-inline
              input#inlineRadio2(type='radio', name='inlineRadioOptions', value='option2')
              |  10 minutes
            label.radio-inline
              input#inlineRadio3(type='radio', name='inlineRadioOptions', value='option3')
              |  15 minutes

            //- .radio
